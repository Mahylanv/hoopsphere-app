  "cli": {
    "version": ">= 16.30.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_EMARQUE_URL": "https://hoopsphere-parser.onrender.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "20.19.4",
      "env": {
        "EXPO_PUBLIC_EMARQUE_URL": "https://hoopsphere-parser.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
